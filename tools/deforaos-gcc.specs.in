%rename cc1 old_cc1
%rename lib old_lib
%rename link old_link

*cc1:
%(cc1_cpu) -D__DeforaOS__=1 -nostdinc -isystem =@INCLUDEDIR@ %(old_cc1)

*libgcc:
@LIBDIR@/libgcc.a

*startfile:
@LIBDIR@/start%O%s @LIBDIR@/crti%O%s @LIBDIR@/crtbegin%O%s

*endfile:
@LIBDIR@/crtend%O%s @LIBDIR@/crtn%O%s

*lib:
%{!nostdlib:-lc} %(old_lib)

*link:
-nostdlib -dynamic-linker @LIBDIR@/ld.so -L =@LIBDIR@ %(old_link)
